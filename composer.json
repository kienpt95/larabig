{
    "name": "smartosc/larabig",
    "description": "BigCommerce package for Laravel",
    "type": "library",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "authors": [
        {
            "name": "Kien Pham",
            "email": "kienpt2@smartosc.com"
        }
    ],
    "autoload": {
        "psr-4": {"Smartosc\\LaraBig\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Smartosc\\LaraBig\\Test\\": "tests"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smartosc\\LaraBig\\LaraBigProvider"
            ]
        }
    },

    "minimum-stability": "dev"
}
